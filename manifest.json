{
    "name": "Screen to Text",
    "description": "Copies text in selected area to clipboard",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_title": "Copy to clipboard"
    },
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "src/background.js", "type": "module"
    }
    
  }